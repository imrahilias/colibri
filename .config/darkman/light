! URxvt (case sensitive?)
! ttps://wiki.archlinux.org/index.php/Rxvt-unicode

! fake transparency:
! fade the text by the given percentage when focus is lost:
! URxvt.*fading: 30
URxvt.*transparent: false
! shading; 0 to 99 darkens, 101 to 200 lightens:
!URxvt.*shading: 10
URxvt.*buffered: true

! real transparency:
! URxvt.*depth: 32
! URxvt.*background: [80]#000000
! URxvt.*background: rgba:0000/0000/0000/c900

URxvt.*foreground: #000000
URxvt.*background: #FAF0E6
URxvt.*cursorColor: #800080
URxvt.*cursorColor2: #800080
URxvt.*pointerColor: #800080
URxvt.*pointerColor2: #800080

! URxvt.*underlineColor:
! URxvt.*troughColor:
! URxvt.*highlightColor:
! URxvt.*highlightTextColor:
URxvt.*scrollColor: #A9A9A9


